<% if collection.any? %>
    <ul class="list-of-entities">
      <% collection.each do |entity| %>
        <li data-id="<%= entity.id %>">
          <%= render partial: 'admin/privilege_groups/entity/in_list', locals: { entity: entity } %>
        </li>
      <% end %>
    </ul>
<% else %>
    <%= render 'shared/nothing_found' %>
<% end %>
